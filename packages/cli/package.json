{
  "name": "memsearch-cli",
  "version": "1.0.5",
  "description": "CLI tool for memsearch-ts - semantic memory search",
  "license": "MIT",
  "author": "memsearch team",
  "type": "module",
  "bin": {
    "memsearch": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "API.md"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "memsearch-core": "workspace:*",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "semantic-search",
    "vector-database",
    "milvus",
    "markdown",
    "memory",
    "rag"
  ]
}
